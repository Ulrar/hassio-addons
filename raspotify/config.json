{
  "name": "Raspotify",
  "version": "dev",
  "slug": "raspotify",
  "panel_icon": "mdi:music-circle",
  "description": "Spotify Connect client for the Raspberry Pi that Just Works",
  "url": "https://github.com/Ulrar/hassio-addons",
  "stage": "experimental",
  "ingress": false,
  "startup": "application",
  "arch": [
    "aarch64",
    "armv7"
  ],
  "boot": "auto",
  "apparmor": true,
  "hassio_api": false,
  "auth_api": false,
  "hassio_role": "default",
  "audio": true,
  "host_network": true,
  "host_dbus": true,
  "host_ipc": false,
  "ports": {},
  "ports_description": {},
  "map": [
    "share:rw"
  ],
  "options": {
    "name": "Raspotify",
    "bitrate": 160,
    "volume_normalisation": true,
    "backend": "alsa"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "name": "str",
    "bitrate": "list(96|160|320)",
    "volume_normalisation": "bool",
    "username": "str?",
    "password": "password?",
    "backend": "list(alsa|pipe)",
    "device": "match(^/|(/[\\w-]+)+$)?",
    "extra_options": "str?"
  }
}
